{
  "name": "neptune",
  "version": "1.0.0",
  "description": "web based fluid simulation playground",
  "browserslist": "defaults",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "parcel build html/*.html",
    "check": "tsc",
    "doc": "typedoc --entryPointStrategy expand src/ types/",
    "format": "prettier --write html/ src/ styles/",
    "start": "parcel html/*.html"
  },
  "author": "Luca Chackalackal",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.12.0",
    "dotenv": "^16.4.5",
    "parcel": "^2.11.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typedoc": "^0.26.10",
    "typescript": "^5.6.3"
  }
}
